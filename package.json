        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.5.0",
        "jest-junit": "^6.3.0",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "react-router-dom": "^5.1.2",
