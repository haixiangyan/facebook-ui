{
    "name": "facebook-ui",
    "version": "0.1.3",
    "description": "A UI library is built for Facebook, using React.",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.dev.js",
        "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
        "test": "cross-env NODE_ENV=test jest --config=jest.config.js --runInBand",
        "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT=./test-results/jest/results.xml jest --config=jest.config.ci.js"
    },
    "files": [
        "/dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Haixiang6123/facebook-ui.git"
    },
    "keywords": [
        "facebook",
        "ui",
        "ui-kit",
        "react",
        "ui-library"
    ],
    "author": "Haixiang",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Haixiang6123/facebook-ui/issues"
    },
    "homepage": "https://github.com/Haixiang6123/facebook-ui#readme",
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "@types/enzyme": "^3.9.1",
        "@types/jest": "^24.0.11",
        "@types/react": "^16.8.8",
        "@types/react-dom": "^16.8.3",
        "@types/react-router-dom": "^5.1.2",
        "@types/react-test-renderer": "^16.8.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
